{
  "name": "simple-phonebook",
  "version": "1.0.0",
  "description": "A phonebook for storing and organizing contacts",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run build && node ./server/build/",
    "build": "npx babel server/src --out-dir server/build --copy-files",
    "client:dev": "webpack-dev-server --open",
    "client:prod": "webpack --env=production -p --progress --colors",
    "server-dev": "babel-watch ./server/src/",
    "server-prod": "npx babel server/src --watch --out-dir server/build --copy-files",
    "reset:db": "sequelize db:migrate:undo:all && sequelize db:migrate"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "body-parser": "^1.18.2",
    "css-loader": "^0.28.8",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.2",
    "prop-types": "^15.7.2",
    "react": "^16.2.0",
    "react-dom": "^16.14.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "sequelize": "^5.8.6",
    "style-loader": "^0.19.1",
    "webpack": "^3.10.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-watch": "^2.0.7",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "redux-devtools-extension": "^2.13.2",
    "sequelize-cli": "^6.2.0",
    "webpack-dev-server": "^2.11.5"
  }
}
